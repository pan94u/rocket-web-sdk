{
  "name": "rocket-web-sdk",
  "version": "0.1.0",
  "description": "A frontend monitor for web.",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "lint": "eslint --fix ./src && prettier --write ./src",
    "test": "nyc mocha --require ts-node/register test/*.spec.ts",
    "coverage": "nyc report --reporter=lcov",
    "esm": "rm -rf ./esm && rm -rf types && tsc -p tsconfig.esm.json",
    "cjs": "rm -rf ./cjs && tsc -p tsconfig.cjs.json",
    "build": "npm run esm && npm run cjs",
    "prepublishOnly": "npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pan94u/rocket-web-sdk"
  },
  "keywords": [],
  "author": {
    "name": "pan9",
    "email": "438063442@qq.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.2.22",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.10",
    "chai": "^4.3.4",
    "mocha": "^9.1.3",
    "nyc": "^15.1.0",
    "prettier": "^2.5.0",
    "source-map-support": "^0.5.19",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "tslib": "^2.3.1"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "include": [
      "src"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
}
